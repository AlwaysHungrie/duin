{
  "name": "duin-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typechain": "typechain --target ethers-v6 --out-dir src/contractTypes '../contracts/out/PrivateMarket.sol/PrivateMarket.json'",
    "pm2:build": "npm run build",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop duin-frontend",
    "pm2:restart": "pm2 restart duin-frontend",
    "pm2:delete": "pm2 delete duin-frontend",
    "pm2:logs": "pm2 logs duin-frontend",
    "pm2:monit": "pm2 monit",
    "pm2:build:start": "npm run pm2:build && npm run pm2:start",
    "build:memory-safe": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:low-memory": "NODE_OPTIONS='--max-old-space-size=1024 --max-semi-space-size=128' next build",
    "build:single-thread": "NODE_OPTIONS='--max-old-space-size=4096' next build --no-lint"
  },
  "dependencies": {
    "@privy-io/react-auth": "^3.3.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethers": "^6.15.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "pm2": "^5.3.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typechain": "^8.3.2",
    "typescript": "^5"
  }
}