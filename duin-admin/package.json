{
  "name": "duin-admin",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec \"tsx src/index.ts\"",
    "test": "node dist/test-connection.js",
    "test:connection": "node test-connection.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "lint": "echo \"Linting not configured\"",
    "type-check": "tsc --noEmit",
    "typechain": "typechain --target ethers-v6 --out-dir src/contractTypes ../contracts/out/PrivateMarket.sol/PrivateMarket.json && node fix-es-modules.cjs",
    "generate-types": "npm run typechain && npm run build",
    "pm2:build": "npm run build",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop duin-admin",
    "pm2:restart": "pm2 restart duin-admin",
    "pm2:delete": "pm2 delete duin-admin",
    "pm2:logs": "pm2 logs duin-admin",
    "pm2:monit": "pm2 monit",
    "pm2:build:start": "npm run pm2:build && npm run pm2:start"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.13.2",
    "express": "^5.1.0",
    "graphql-request": "^7.3.1"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.8.1",
    "nodemon": "^3.1.10",
    "pm2": "^5.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typechain": "^8.3.2",
    "typescript": "^5.9.3"
  }
}
